parameters:
    hosts: [[www.shop2tout.com, www.shop2tout.local]]
    logFile: log/test.log
    testStack: curl
    beginLine: 0
    numberOfLine: 30
    logParserClass: \Log2Test\Apache2LogParser
    logFormat: '%h %l %u %t \"%r\" %>s %b'
    extensions_allowed: ['*']
    browsers: ['*chrome']
    removeDuplicateUrl: true
    pauseBetweenTests: 0
    encodedUrls: false
    enabledScreenshot: true
    log2testVersion: V1.2
    currentTestSuiteId: 1
    numberOfFileByTestSuite: 30
    testResultFormat: xml
